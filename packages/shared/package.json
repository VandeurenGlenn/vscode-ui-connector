{
	"name": "shared",
	"version": "1.0.0",
	"type": "module",
     "author": "Valentin Degenne <vdegenne@gmail.com>",
     "license": "MIT",
	"exports": {
		".": {
			"import": "./lib/index.js",
			"types": "./lib/index.d.ts"
		},
		"./constants": {
			"import": "./lib/constants.js",
			"types": "./lib/constants.d.ts"
		}
	},
	"scripts": {
		"build": "wireit"
	},
	"wireit": {
		"build": {
			"command": "tsc --pretty",
			"clean": "if-file-deleted",
			"files": [
				"tsconfig.json",
				"./src"
			],
			"output": [
				"./lib"
			]
		}
	}
}
